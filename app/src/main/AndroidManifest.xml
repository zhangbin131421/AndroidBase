        <activity android:name=".activity.handle.OrderHandleActivity_" android:label="工单处理"></activity>
        <activity android:name=".activity.handle.BusinessAuditeActivity_" android:label="营业普查"></activity>
        <activity android:name=".activity.handle.StopStartElectricActivity_" android:label="停复电"></activity>
